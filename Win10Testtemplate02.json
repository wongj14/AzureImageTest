{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
     
 	 	"publisher": {
			"type": "string",
			"defaultValue": "MicrosoftWindowsDesktop"
		},
		"offer": {
			"type": "string",
			"defaultValue": "windows-10-1909-vhd-client-office-prod-stage"
		},
		"sku": {
			"type": "string",
			"defaultValue": "19h2-evd-o365pp"
		},  
	"location": {
            "value": "westus"
        },
        "networkInterfaceName": {
            "value": "win10officepp01538"
        },
        "networkSecurityGroupName": {
            "value": "Win10OfficePP01-nsg"
        },
        "networkSecurityGroupRules": {
            "value": [
                {
                    "name": "RDP",
                    "properties": {
                        "priority": 300,
                        "protocol": "TCP",
                        "access": "Allow",
                        "direction": "Inbound",
                        "sourceAddressPrefix": "*",
                        "sourcePortRange": "*",
                        "destinationAddressPrefix": "*",
                        "destinationPortRange": "3389"
                    }
                }
            ]
        },
        "subnetName": {
            "value": "default"
        },
        "virtualNetworkName": {
            "value": "RG_WestUS_AzureImageBuilder-vnet"
        },
        "addressPrefixes": {
            "value": [
                "10.0.1.0/24"
            ]
        },
        "subnets": {
            "value": [
                {
                    "name": "default",
                    "properties": {
                        "addressPrefix": "10.0.1.0/24"
                    }
                }
            ]
        },
        "publicIpAddressName": {
            "value": "Win10OfficePP01-ip"
        },
        "publicIpAddressType": {
            "value": "Dynamic"
        },
        "publicIpAddressSku": {
            "value": "Basic"
        },
        "virtualMachineName": {
            "value": "Win10OfficePP01"
        },
        "virtualMachineRG": {
            "value": "RG_WestUS_AzureImageBuilder"
        },
        "osDiskType": {
            "value": "Standard_LRS"
        },
        "virtualMachineSize": {
            "value": "Standard_A2_v2"
        },
        "adminUsername": {
            "value": "soshk"
        },
        "adminPassword": {
            "value": null
        }
    }
}
